<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LichSuSuaChuaMayTinhActivity">
    <android.support.v7.widget.Toolbar
        app:title="@string/lichsu"
        android:id="@+id/tblichsusuachuamaytinh"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:background="@drawable/background_toolbar"
        app:titleTextColor="@color/colorWhite">
    </android.support.v7.widget.Toolbar>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/colorWhite">

        <ExpandableListView
            android:visibility="visible"
            android:dividerHeight="1dp"
            android:divider="@color/colorLine"
            android:childDivider="@color/colorbackground"
            android:listSelector="@color/colorSelect"
            android:id="@+id/expLvLichSu"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>


    </LinearLayout>

</LinearLayout>